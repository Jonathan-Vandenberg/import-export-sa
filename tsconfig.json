{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "types": ["node", "@types/parse-json", "@types/webidl-conversions", "@types/whatwg-url"],
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/components/covidApi.jsx",
    "pages/scraper..jsx",
    "pages/decor.tsx",
    "pages/api/category-images.js"
, "src/components/decor/category-selector.jsx"  ],
  "exclude": ["node_modules"]
}
